{
  "name": "skribbltypo",
  "description": "",
  "version": "27.0.0",
  "manifest_version": 3,
  "icons": {
    "16": "icons/16Fit.png",
    "32": "icons/32CircleFit.png",
    "128": "icons/128MaxFit.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/128MaxFit.png"
  },
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/loader.ts-loader.js",
        "assets/content.ts-loader.js",
        "assets/css-urls.ts-loader.js"
      ],
      "matches": [
        "https://skribbl.io/*"
      ],
      "run_at": "document_start",
      "css": [
        "assets/loader.css",
        "assets/content.css"
      ]
    },
    {
      "js": [
        "assets/gif-renderer.worker.ts-loader.js"
      ],
      "matches": [
        "https://skribbl.io/never"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "https://skribbl.io/*"
      ],
      "resources": [
        "icons/16Fit.png",
        "icons/32CircleFit.png",
        "icons/128MaxFit.png",
        "img/**",
        "assets/*.worker.ts.js",
        "**/*.js.map",
        "gamePatch.js",
        "assets/requiredQuerySelector.js",
        "assets/loader.ts.js",
        "assets/index.js",
        "assets/typed-worker.js",
        "assets/content.ts.js",
        "assets/css-urls.ts.js",
        "assets/gif-renderer.worker.ts.js"
      ],
      "use_dynamic_url": false
    }
  ],
  "permissions": [
    "storage",
    "activeTab"
  ],
  "version_name": "27.0.0 beta-crx 1b14cd6"
}
